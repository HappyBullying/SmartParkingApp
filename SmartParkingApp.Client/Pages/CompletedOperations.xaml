<Page x:Class="SmartParkingApp.Client.Pages.CompletedOperations"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:SmartParkingApp.Client.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="CompletedOperations">
    <Grid Background="Lavender">
        <ListView Name="ParkingSession">
            <ListView.View>
                <GridView>
                    <GridViewColumn>TicketNumber</GridViewColumn>
                    <GridViewColumn>EntryDate</GridViewColumn>
                    <GridViewColumn>PaymentDate</GridViewColumn>
                    <GridViewColumn>ExitDate</GridViewColumn>
                    <GridViewColumn>TotalPayment</GridViewColumn>
                    <GridViewColumn>CarPlateNumber</GridViewColumn>
                </GridView>
            </ListView.View>
        </ListView>
    </Grid>
</Page>
